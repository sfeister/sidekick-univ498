	field(DTYP,"stream")
	field(ZNAM,"False")
	field(ONAM,"True")
    field(OUT,"@belle.proto setBuzz belle_ino")
    }

record(stringin,"BELLE:debug"){
	field(DTYP,"stream")
	field(INP,"@belle.proto debug belle_ino")
}
