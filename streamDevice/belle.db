record(stringin,"BELLE:info"){
    field (DESC, "Get information about the device")
	field(DTYP,"stream")
	field(INP,"@belle.proto info belle_ino")
    field(SCAN, "10 second")
    field(PINI, "YES")
}

record(bo,"BELLE:BUZZ:ON:set"){
	field(DTYP,"stream")
	field(ZNAM,"False")
	field(ONAM,"True")
    field(OUT,"@belle.proto Buzzer belle_ino")
    }

record(bi,"BELLE:BUZZ:ON:get"){
	field(DTYP,"stream")
    field(SCAN,"1 second")
	field(ZNAM,"False")
	field(ONAM,"True")
    field(INP,"@belle.proto BuzzerQ belle_ino")
    }

record(ao,"BELLE:BUZZ:FREQ:set"){
	field(DTYP,"stream")
    field(OUT,"@belle.proto BuzzerFrequency belle_ino")
    }

record(ai,"BELLE:BUZZ:FREQ:get"){
	field(DTYP,"stream")
    field(SCAN,"1 second")
    field(INP,"@belle.proto BuzzerFrequencyQ belle_ino")
    }

record(stringin,"BELLE:debug"){
	field(DTYP,"stream")
	field(INP,"@belle.proto debug belle_ino")
}
