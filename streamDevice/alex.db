record(stringin,"ALEX:info"){
    field (DESC, "Get information about the device")
	field(DTYP,"stream")
	field(INP,"@alex.proto info alex_ino")
    field(SCAN, "10 second")
    field(PINI, "YES")
}

record(longout,"ALEX:POS:CYCLE:set"){
	field(DTYP,"stream")
	field(DRVL,"0")
	field(DRVH,"30")  
	field(OUT,"@alex.proto PositionCycle alex_ino")
}

record(longout,"ALEX:POS:set"){
	field(DTYP,"stream")
	field(DRVL,"0")
	field(DRVH,"3")
	field(OUT,"@alex.proto PositionSet alex_ino")
}

record(longin,"ALEX:POS:get"){
	field(DTYP,"stream")
    field(SCAN,"1 second")
	field(INP,"@alex.proto PositionGet alex_ino")
}

record(stringin,"ALEX:debug"){
	field(DTYP,"stream")
	field(INP,"@alex.proto debug alex_ino")
}
